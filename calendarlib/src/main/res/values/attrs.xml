<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DayView">
        <attr name="day_text" format="string" />
    </declare-styleable>

    <declare-styleable name="CalendarView">
        <attr name="nb_month_in_future" format="integer" />
        <attr name="nb_month_in_past" format="integer" />
        <attr name="selected_color" format="color"/>
        <attr name="disabled_color" format="color" />
        <attr name="selected_text_color" format="color" />
        <attr name="cv_pick_mode" format="enum">
            <enum name="RANGE" value="0" />
            <enum name="SINGLE" value="1" />
        </attr>
    </declare-styleable>
</resources>